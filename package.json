{
  "name": "enscribe",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:prod": "cross-env NODE_ENV=production node generate-config.js && expo start",
    "start:dev": "cross-env NODE_ENV=development node generate-config.js && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "xcode:dev": "cross-env NODE_ENV=development node generate-config.js && expo prebuild --clean && echo '✅ Ready for Xcode development build'",
    "xcode:prod": "cross-env NODE_ENV=production node generate-config.js && expo prebuild --clean && echo '✅ Ready for Xcode production build'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@supabase/supabase-js": "^2.56.0",
    "dotenv": "^17.2.1",
    "expo": "~53.0.22",
    "expo-av": "^15.1.7",
    "expo-clipboard": "^7.1.5",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-keep-awake": "^14.1.4",
    "expo-network": "^7.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "cross-env": "^10.0.0"
  },
  "private": true
}
